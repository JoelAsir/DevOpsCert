pipeline {
    agent any
    tools {
        maven 'mvn_home'
    }
    stages {
        stage('SCM-Checkout') {
            steps {
                 git credentialsId: 'git-credentials', url: 'https://github.com/JoelAsir/DevOpsCert.git'
            }
        }  
        stage('build') {
            steps {
                echo 'This is Build Stage'
            }
        }
        stage('test') {
            steps {
                echo 'This is Test Stage'
            }
        }
        stage('deploy') {
            steps {
                echo 'This is Deploy Stage'
            }
        }
    }
}
